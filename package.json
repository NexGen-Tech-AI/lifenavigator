{
  "name": "lifenavigator",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed-production.ts"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "@types/bcryptjs": "3.0.0"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "setup": "bash ./setup.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:pg:up": "docker-compose -f docker-compose.postgres.yml up -d",
    "docker:pg:down": "docker-compose -f docker-compose.postgres.yml down",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:migrate-to-pg": "bash ./scripts/migrate-to-postgres.sh",
    "db:ensure-demo": "node scripts/init-demo-account.js",
    "prepare": "husky install",
    "postinstall": "prisma generate --no-engine",
    "env:generate": "bash ./scripts/generate-env.sh",
    "env:generate:dev": "bash ./scripts/generate-env.sh --env dev",
    "env:generate:staging": "bash ./scripts/generate-env.sh --env staging",
    "env:generate:prod": "bash ./scripts/generate-env.sh --env prod",
    "env:validate": "node scripts/validate-env.js",
    "auth:fix": "node scripts/fix-auth.js",
    "db:diagnose": "node scripts/db-diagnostic.js",
    "vercel:setup": "node scripts/vercel-setup.js",
    "vercel:postgres:setup": "ts-node scripts/setup-vercel-postgres.ts",
    "create:demo-account": "ts-node scripts/create-demo-account.ts",
    "db-deploy": "prisma migrate deploy && prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-pg": "^5.10.2",
    "@prisma/client": "6.8.2",
    "@tanstack/react-query": "^5.74.11",
    "@types/lodash": "^4.17.16",
    "bcryptjs": "^3.0.2",
    "classnames": "^2.5.1",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^12.9.2",
    "jose": "^5.2.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.507.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.6",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "plaid": "^34.0.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.2.3",
    "react-plaid-link": "^4.0.1",
    "recharts": "^2.15.3",
    "stream-browserify": "^3.0.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.11.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "15.3.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "6.8.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
