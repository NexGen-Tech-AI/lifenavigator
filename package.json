{
  "name": "lifenavigator",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed-production.ts"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "@types/bcryptjs": "3.0.0"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/__tests__",
    "test:integration": "jest --testPathPattern=src/__tests__/api",
    "test:security": "jest --testPathPattern=src/__tests__/security",
    "test:performance": "jest --testPathPattern=src/__tests__/performance",
    "test:a11y": "jest --testPathPattern=src/__tests__/accessibility",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:soc2": "jest --testPathPattern=src/__tests__/soc2",
    "test:soc2:security": "jest --testPathPattern=src/__tests__/soc2/security",
    "test:soc2:availability": "jest --testPathPattern=src/__tests__/soc2/availability",
    "test:soc2:integrity": "jest --testPathPattern=src/__tests__/soc2/processing-integrity",
    "test:soc2:confidentiality": "jest --testPathPattern=src/__tests__/soc2/confidentiality",
    "test:soc2:privacy": "jest --testPathPattern=src/__tests__/soc2/privacy",
    "test:soc2:audit": "jest --testPathPattern=src/__tests__/soc2/audit-monitoring",
    "soc2:report": "jest --testPathPattern=src/__tests__/soc2/run-compliance --verbose",
    "soc2:pre-deploy": "npm run test:soc2 && npm run soc2:validate",
    "soc2:validate": "node scripts/validate-soc2-compliance.js",
    "test:compliance": "jest --testPathPattern=src/__tests__/compliance",
    "test:compliance:hipaa": "jest --testPathPattern=src/__tests__/compliance/hipaa",
    "test:compliance:gdpr": "jest --testPathPattern=src/__tests__/compliance/gdpr",
    "test:compliance:pci": "jest --testPathPattern=src/__tests__/compliance/pci-dss",
    "test:compliance:ccpa": "jest --testPathPattern=src/__tests__/compliance/ccpa",
    "test:compliance:iso": "jest --testPathPattern=src/__tests__/compliance/iso27001",
    "test:compliance:nist": "jest --testPathPattern=src/__tests__/compliance/nist",
    "test:compliance:unified": "jest --testPathPattern=src/__tests__/compliance/unified",
    "compliance:report": "jest --testPathPattern=src/__tests__/compliance/unified-compliance --verbose",
    "compliance:all": "npm run test:soc2 && npm run test:compliance",
    "compliance:validate": "npm run test:compliance:unified && npm run soc2:validate",
    "setup": "bash ./setup.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:pg:up": "docker-compose -f docker-compose.postgres.yml up -d",
    "docker:pg:down": "docker-compose -f docker-compose.postgres.yml down",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:migrate-to-pg": "bash ./scripts/migrate-to-postgres.sh",
    "db:ensure-demo": "node scripts/init-demo-account.js",
    "prepare": "husky install",
    "postinstall": "prisma generate --no-engine",
    "env:generate": "bash ./scripts/generate-env.sh",
    "env:generate:dev": "bash ./scripts/generate-env.sh --env dev",
    "env:generate:staging": "bash ./scripts/generate-env.sh --env staging",
    "env:generate:prod": "bash ./scripts/generate-env.sh --env prod",
    "env:validate": "node scripts/validate-env.js",
    "auth:fix": "node scripts/fix-auth.js",
    "auth:validate": "node scripts/validate-auth.js",
    "db:diagnose": "node scripts/db-diagnostic.js",
    "vercel:setup": "node scripts/vercel-setup.js",
    "vercel:postgres:setup": "ts-node scripts/setup-vercel-postgres.ts",
    "create:demo-account": "ts-node scripts/create-demo-account.ts",
    "db-deploy": "prisma migrate deploy && prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.826.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^0.9.0",
    "@prisma/adapter-pg": "^5.10.2",
    "@prisma/client": "6.8.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.74.11",
    "@types/lodash": "^4.17.16",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "classnames": "^2.5.1",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.9.2",
    "jose": "^5.2.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.1.0",
    "next": "15.3.1",
    "next-intl": "^4.1.0",
    "next-themes": "^0.4.6",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "plaid": "^34.0.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.2.3",
    "react-plaid-link": "^4.0.1",
    "recharts": "^2.15.3",
    "stream-browserify": "^3.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.11.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^2.0.1",
    "eslint": "^9",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "15.3.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "6.8.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
