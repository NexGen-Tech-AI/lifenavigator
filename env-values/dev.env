# Development environment values
# These values will be used when generating the .env file for development

# Database configuration
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/lifenavigator?schema=public"

# NextAuth configuration
NEXTAUTH_SECRET="lifenavigator-super-secure-secret-key-12345"
NEXTAUTH_URL="http://localhost:3000"

# Field-level encryption configuration
ENABLE_FIELD_ENCRYPTION="true"
ENCRYPTION_MASTER_KEY="lifenavigator-master-key-for-field-encryption-32chars"
ENCRYPTION_SALT="lifenavigator-encryption-salt-value-12345"

# Environment settings
NODE_ENV="development"
LOG_LEVEL="debug"
ENABLE_API_LOGGING="true"