# Production environment values example
# IMPORTANT: Copy this file to prod.env and fill in the actual values
# DO NOT commit the actual production values to version control

# Database configuration
DATABASE_URL="postgresql://username:password@production-db-host:5432/lifenavigator?schema=public"

# NextAuth configuration
NEXTAUTH_SECRET="replace-with-production-secret-min-32-chars"
NEXTAUTH_URL="https://app.lifenavigator.example.com"

# Field-level encryption configuration
ENABLE_FIELD_ENCRYPTION="true"
ENCRYPTION_MASTER_KEY="replace-with-production-encryption-key-min-32-chars"
ENCRYPTION_SALT="replace-with-production-salt-value"

# AWS Configuration for document storage
AWS_REGION="us-east-1"
AWS_ACCESS_KEY_ID="replace-with-aws-access-key"
AWS_SECRET_ACCESS_KEY="replace-with-aws-secret-key"
S3_BUCKET_NAME="lifenavigator-documents-prod"

# Document encryption
DOCUMENT_ENCRYPTION_KEY="replace-with-document-encryption-key-min-32-chars"

# Environment settings
NODE_ENV="production"
LOG_LEVEL="error"
ENABLE_API_LOGGING="false"